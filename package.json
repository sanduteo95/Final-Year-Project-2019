{
  "name": "futamura-js",
  "version": "1.0.0",
  "description": "Application of the first Futamura Projections on a toy lambda calculus language",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "preferGlobal": true,
  "bin": {
    "futamura-js": "./index.js"
  },
  "scripts": {
    "test": "npm run test-lambda; npm run test-efsd",
    "test-lambda": "mocha ./test/toyLambda.js",
    "test-efsd": "mocha ./test/EFSD.js",
    "create-report": "npm run create-lambda-report; npm run create-efsd-report",
    "create-strategy-report": "caffeinate node --max-old-space-size=120000 --stack-size=100000 index.js -cr",
    "create-lambda-report": "caffeinate node --max-old-space-size=120000 --stack-size=100000 index.js -r",
    "create-efsd-report": "caffeinate node --max-old-space-size=120000 --stack-size=100000 index.js -r -g",
    "v1-goi-machine": "webpack --config ./lib/EFSD/UI/v1/webpack.config.js --display-error-details; webpack --env=v1 --config ./lib/EFSD/UI/webpack.config.js",
    "v2-goi-machine": "webpack --config ./lib/EFSD/UI/v2/webpack.config.js --display-error-details; node ./lib/EFSD/UI/v2/index.js; webpack --env=v2 --config ./lib/EFSD/UI/webpack.config.js",
    "v3-goi-machine": "node ./lib/EFSD/UI/v3/index.js; webpack --env=v3 --config ./lib/EFSD/UI/webpack.config.js",
    "term-goi-machine": "webpack --config ./lib/EFSD/term/webpack.config.js --display-error-details",
    "build-prepack": "rm ./lib/prepack/EFSD.min.js; rm ./lib/prepack/toyLambda.min.js; webpack --config ./lib/prepack/webpack.config.js --display-modules"
  },
  "repository": {
    "type": "git",
    "url": "https://git-teaching.cs.bham.ac.uk/mod-ug-proj-2018/tas458.git"
  },
  "author": "Teodora Sandu",
  "license": "UNLICENSED",
  "dependencies": {
    "benchmark": "^2.1.4",
    "child_process": "^1.0.2",
    "commander": "^2.19.0",
    "create-html": "^4.1.0",
    "fast-csv": "^2.4.1",
    "fs": "0.0.1-security",
    "google-closure-compiler": "^20190121.0.0",
    "jison": "^0.4.18",
    "jquery": "^3.3.1",
    "jsdom": "^13.0.0",
    "lex": "^1.7.9",
    "microtime": "^2.1.8",
    "path": "^0.12.7",
    "prepack": "^0.2.54",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "exports-loader": "^0.7.0",
    "expose-loader": "^0.7.5",
    "imports-loader": "^0.8.0",
    "mocha": "^5.2.0",
    "prepack-webpack-plugin": "^1.1.2",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  }
}
